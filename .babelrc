{
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": [
            "ios >= 7",
            "android >= 4.4"
          ]
        }
      }
    ],
    "stage-0"
  ],
  "plugins": [
    "transform-runtime",
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "syntax-dynamic-import",
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ]
  ]
}
